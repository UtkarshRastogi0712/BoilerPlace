{
  "name": "boilerplace",
  "displayName": "Boilerplace",
  "description": "A handy extension for all things boilerplate",
  "repository": {
    "type": "github",
    "url": "https://github.com/UtkarshRastogi0712/BoilerPlace"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "icon": "./BoilerplaceLogo.png",
  "categories": [
    "Snippets",
    "Extension Packs"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "boilerplace.helloWorld",
        "title": "Boilerplace Hello World"
      },
      {
        "command": "boilerplace.init",
        "title": "Boilerplace init"
      },
      {
        "command": "boilerplace.create",
        "title": "Boilerplace create"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "joi": "^17.11.0",
    "raw-loader": "^4.0.2"
  }
}
